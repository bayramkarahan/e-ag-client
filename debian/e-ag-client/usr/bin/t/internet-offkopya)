#!/bin/bash
iptables -P OUTPUT ACCEPT
iptables -F OUTPUT
ip6tables -P OUTPUT ACCEPT
ip6tables -F OUTPUT

iptables -A OUTPUT -p tcp --dport 80 -j DROP
iptables -A OUTPUT -p tcp --dport 443 -j DROP
ip6tables -A OUTPUT -p tcp --dport 80 -j DROP
ip6tables -A OUTPUT -p tcp --dport 443 -j DROP

iptables -A OUTPUT -p udp --dport 80 -j DROP
iptables -A OUTPUT -p udp --dport 443 -j DROP
ip6tables -A OUTPUT -p udp --dport 80 -j DROP
ip6tables -A OUTPUT -p udp --dport 443 -j DROP
